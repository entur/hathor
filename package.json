{
  "name": "hathor",
  "private": true,
  "version": "0.24.0",
  "type": "module",
  "scripts": {
    "predev": "run-p --race copy:theme copy:config",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "copy:theme": "ncp .github/environments/theme-localhost.json public/theme-dev.json",
    "copy:config": "ncp .github/environments/config-localhost.json public/config.json",
    "lint": "eslint .",
    "check": "prettier . --check",
    "format": "prettier --write .",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "e2e:auth": "E2E_SUITE=auth npx playwright test",
    "e2e:no-auth": "E2E_SUITE=no-auth npx playwright test"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@mui/x-data-grid": "^8.2.0",
    "@types/papaparse": "^5.5.2",
    "fast-xml-parser": "^5.3.0",
    "graphql": "^16.11.0",
    "graphql-request": "^7.3.1",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "oidc-client-ts": "^3.2.1",
    "papaparse": "^5.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.5.2",
    "react-oidc-context": "^3.3.0",
    "react-router-dom": "^6.30.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@playwright/test": "^1.58.0",
    "@types/node": "^22.15.12",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "ncp": "^2.0.0",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.4.1",
    "vitest": "^4.0.18"
  }
}
